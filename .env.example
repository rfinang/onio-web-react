# Next.js Frontend Environment Variables
# Copy this file to .env and fill in your values

# Strapi CMS API URLs
NEXT_PUBLIC_DOMAIN_API=http://localhost:1337/api/
HOST_URL=http://localhost:1337

# Redis Configuration
REDIS_PORT=6379
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=your_redis_password

# Webhook Configuration  
WEBHOOK_HANDLER_KEY=your_webhook_key

# Server Configuration
SERVER_TIMEZONE=Europe/Oslo

# Cache Management
CLEAR_ALL_CACHE_KEY=clear_cache_key_for_quick_testing

# Third-party Services (Optional)
# CLOUDINARY_CLOUD_NAME=your_cloudinary_name
# CLOUDINARY_API_KEY=your_cloudinary_key
# CLOUDINARY_API_SECRET=your_cloudinary_secret

# Analytics & Monitoring (Optional)
# GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# HOTJAR_ID=your_hotjar_id